    <script>
        $(document).ready(function () {
            updateDateTime();
            updateNotificationBadge();
            $('#sidenav-main').find('[href="' + window.location.pathname + '"]').addClass('active');

        })

        function updateDateTime() {
            var now = new Date();
            var options = {
                /*weekday: 'long',*/
                year: 'numeric',
                month: 'long',
                day: 'numeric',
            };
            var formattedDateTime = now.toLocaleString('en-US', options);
            $("#datetime").text(formattedDateTime);
        }
        setInterval(updateDateTime, 1000);
        function updateNotificationBadge() {
            $.ajax({
                url: '/Home/GetNotificationCount',
                type: 'GET',
                success: function (notificationCount) {
                    debugger
                    $('#notificationBadge').text(notificationCount);
                },
                error: function () {
                    console.log('Error occurred while retrieving the notification count.');
                }
            });
        }

